Icon from '@coreui/icons-react/lib/CIcon';;\n\nconst TheHeaderDropdownNotif = () => {\n  const itemsCount = 5;\n  return (\n    <CDropdown inNav className=\"c-header-nav-item mx-2\">\n      <CDropdownToggle className=\"c-header-nav-link\" caret={false}>\n        <CIcon name=\"cil-bell\" />\n        <CBadge shape=\"pill\" color=\"danger\">\n          {itemsCount}\n        </CBadge>\n      </CDropdownToggle>\n      <CDropdownMenu placement=\"bottom-end\" className=\"pt-0\">\n        <CDropdownItem header tag=\"div\" className=\"text-center\" color=\"light\">\n          <strong>You have {itemsCount} notifications</strong>\n        </CDropdownItem>\n        <CDropdownItem>\n          <CIcon name=\"cil-user-follow\" className=\"mr-2 text-success\" /> New user registered\n        </CDropdownItem>\n        <CDropdownItem>\n          <CIcon name=\"cil-user-unfollow\" className=\"mr-2 text-danger\" /> User deleted\n        </CDropdownItem>\n        <CDropdownItem>\n          <CIcon name=\"cil-chart-pie\" className=\"mr-2 text-info\" /> Sales report is ready\n        </CDropdownItem>\n        <CDropdownItem>\n          <CIcon name=\"cil-basket\" className=\"mr-2 text-primary\" /> New client\n        </CDropdownItem>\n        <CDropdownItem>\n          <CIcon name=\"cil-speedometer\" className=\"mr-2 text-warning\" /> Server overloaded\n        </CDropdownItem>\n        <CDropdownItem header tag=\"div\" color=\"light\">\n          <strong>Server</strong>\n        </CDropdownItem>\n        <CDropdownItem className=\"d-block\">\n          <div className=\"text-uppercase mb-1\">\n            <small>\n              <b>CPU Usage</b>\n            </small>\n          </div>\n          <CProgress size=\"xs\" color=\"info\" value={25} />\n          <small className=\"text-muted\">348 Processes. 1/4 Cores.</small>\n        </CDropdownItem>\n        <CDropdownItem className=\"d-block\">\n          <div className=\"text-uppercase mb-1\">\n            <small>\n              <b>Memory Usage</b>\n            </small>\n          </div>\n          <CProgress size=\"xs\" color=\"warning\" value={70} />\n          <small className=\"text-muted\">11444GB/16384MB</small>\n        </CDropdownItem>\n        <CDropdownItem className=\"d-block\">\n          <div className=\"text-uppercase mb-1\">\n            <small>\n              <b>SSD 1 Usage</b>\n            </small>\n          </div>\n          <CProgress size=\"xs\" color=\"danger\" value={90} />\n          <small className=\"text-muted\">243GB/256GB</small>\n        </CDropdownItem>\n      </CDropdownMenu>\n    </CDropdown>\n  );\n};\n\nexport default TheHeaderDropdownNotif;\n"]}]}