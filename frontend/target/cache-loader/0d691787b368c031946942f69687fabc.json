AACf,4BACE,oBAAC,SAAD;AAAW,UAAA,IAAI,EAAElC,QAAO,CAACsD,QAAR,CAAiBpB,IAAI,CAACmB,EAAtB;AAAjB,wBACE,oBAAC,WAAD,qBACE,6EADF,CADF,eAIE,oBAAC,SAAD,qBACE;AAAI,UAAA,SAAS,EAAC;AAAd,wBACE;AAAI,UAAA,SAAS,EAAC;AAAd,iCADF,eAEE;AAAI,UAAA,SAAS,EAAC;AAAd,WAA0BnB,IAAI,CAACqB,IAA/B,CAFF,CADF,eAKE;AAAI,UAAA,SAAS,EAAC;AAAd,wBACE;AAAI,UAAA,SAAS,EAAC;AAAd,kCADF,eAEE;AAAI,UAAA,SAAS,EAAC;AAAd,WAA0BrB,IAAI,CAACsB,IAA/B,CAFF,CALF,eASE;AAAI,UAAA,SAAS,EAAC;AAAd,wBACE;AAAI,UAAA,SAAS,EAAC;AAAd,4BADF,eAEE;AAAI,UAAA,SAAS,EAAC;AAAd,WAA0BtB,IAAI,CAACuB,WAA/B,CAFF,CATF,CAJF,CADF;AAqBD;AA1DU;AAnBf,IAJF,eAoFE,oBAAC,WAAD;AACE,IAAA,UAAU,EAAEpD,UADd;AAEE,IAAA,KAAK,EAAEqD,IAAI,CAACC,KAAL,CAAWvD,YAAY,CAACwD,SAAb,GAAyBrD,IAApC,IAA4C,CAFrD;AAGE,IAAA,kBAAkB,EAAE,4BAAAsD,CAAC;AAAA,aAAIvD,aAAa,CAACuD,CAAD,CAAjB;AAAA;AAHvB,IApFF,CAPF,CADF;AAoGD,CAjLD;;AAmLA,eAAe/D,MAAf","sourcesContent":["import React, { useEffect, useState } from 'react';\nimport { CCardBody, CBadge, CButton, CCollapse, CDataTable, CCard, CCardHeader, CRow, CCol, CPagination } from '@coreui/react';\nimport CIcon from '@coreui/icons-react';\nimport { useDispatch, useSelector } from 'react-redux';\n// import { getBranch } from '../customer.api.js';\nimport { useHistory } from 'react-router-dom';\nimport { getBranch } from './customer-branch.api.js';\nimport { globalizedbranchelectors, reset } from './customer-branch.reducer.js';\n\nconst Branch = props => {\n  const [details, setDetails] = useState([]);\n  const { initialState } = useSelector(state => state.branch);\n  const [activePage, setActivePage] = useState(1);\n  const [size, setSize] = useState(20);\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const { selectAll } = globalizedbranchelectors;\n  const branches = useSelector(selectAll);\n  useEffect(() => {\n    dispatch(getBranch());\n    dispatch(reset());\n  }, []);\n\n  useEffect(() => {\n    dispatch(getBranch({ page: activePage - 1, size: size, sort: 'createdDate,desc' }));\n  }, [activePage, size]);\n\n  const toggleDetails = index => {\n    const position = details.indexOf(index);\n    let newDetails = details.slice();\n    if (position !== -1) {\n      newDetails.splice(position, 1);\n    } else {\n      newDetails = [...details, index];\n    }\n    setDetails(newDetails);\n  };\n\n  // Code\tTên cửa hàng/đại lý\tNgười liên lạc\tNăm Sinh\tĐiện thoại\tNhân viên quản lý\tLoại khách hàng\tPhân loại\tSửa\tTạo đơn\n  const fields = [\n    {\n      key: 'order',\n      label: 'STT',\n      _style: { width: '1%' },\n      filter: false,\n    },\n    { key: 'code', label: 'Mã chi nhánh', _style: { width: '15%' } },\n    { key: 'name', label: 'Tên chi nhánh', _style: { width: '15%' } },\n    { key: 'description', label: 'Mô tả', _style: { width: '15%' } },\n    {\n      key: 'show_details',\n      label: '',\n      _style: { width: '1%' },\n      filter: false,\n    },\n  ];\n\n  const getBadge = status => {\n    switch (status) {\n      case 'Active':\n        return 'success';\n      case 'Inactive':\n        return 'secondary';\n      case 'Pending':\n        return 'warning';\n      case 'Banned':\n        return 'danger';\n      default:\n        return 'primary';\n    }\n  };\n  const [currentItems, setCurrentItems] = useState([]);\n  const csvContent = branches.map(item => Object.values(item).join(',')).join('\\n');\n  const csvCode = 'data:text/csv;charset=utf-8,SEP=,%0A' + encodeURIComponent(csvContent);\n  const toCreateCustomer = () => {\n    history.push(`${props.match.url}/new`);\n  };\n\n  const onFilterColumn = value => {\n    dispatch(getBranch({ page: 0, size: size, sort: 'createdDate,desc', ...value }));\n  };\n\n  const toEditBranch = statusId => {\n    history.push(`${props.match.url}/${statusId}/edit`);\n  };\n\n  return (\n    <CCard>\n      <CCardHeader>\n        <CIcon name=\"cil-grid\" /> Danh sách chi nhánh khách hàng\n        <CButton color=\"success\" variant=\"outline\" className=\"ml-3\" onClick={toCreateCustomer}>\n          <CIcon name=\"cil-plus\" /> Thêm mới\n        </CButton>\n      </CCardHeader>\n      <CCardBody>\n        <CButton color=\"primary\" className=\"mb-2\" href={csvCode} download=\"coreui-table-data.csv\" target=\"_blank\">\n          Tải excel (.csv)\n        </CButton>\n        <CDataTable\n          items={branches}\n          fields={fields}\n          columnFilter\n          tableFilter\n          cleaner\n          itemsPerPageSelect={{ label: 'Số lượng trên một trang', values: [20, 30, 50] }}\n          itemsPerPage={size}\n          hover\n          sorter\n          // loading\n          // onRowClick={(item,index,col,e) => console.log(item,index,col,e)}\n          onPageChange={val => console.log('new page:', val)}\n          onPagesChange={val => console.log('new pages:', val)}\n          onPaginationChange={val => setSize(val)}\n          // onFilteredItemsChange={(val) => console.log('new filtered items:', val)}\n          // onSorterValueChange={(val) => console.log('new sorter value:', val)}\n          onTableFilterChange={val => console.log('new table filter:', val)}\n          onColumnFilterChange={onFilterColumn}\n          scopedSlots={{\n            order: (item, index) => <td>{index + 1}</td>,\n            status: item => (\n              <td>\n                <CBadge color={getBadge(item.status)}>{item.status}</CBadge>\n              </td>\n            ),\n            show_details: item => {\n              return (\n                <td className=\"d-flex py-2\">\n                  <CButton\n                    color=\"primary\"\n                    variant=\"outline\"\n                    shape=\"square\"\n                    size=\"sm\"\n                    className=\"mr-3\"\n                    onClick={() => {\n                      toEditBranch(item.id);\n                    }}\n                  >\n                    <CIcon name=\"cil-pencil\" />\n                  </CButton>\n                  <CButton\n                    color=\"primary\"\n                    variant=\"outline\"\n                    shape=\"square\"\n                    size=\"sm\"\n                    onClick={() => {\n                      toggleDetails(item.id);\n                    }}\n                  >\n                    <CIcon name=\"cilZoom\" />\n                  </CButton>\n                </td>\n              );\n            },\n            details: item => {\n              return (\n                <CCollapse show={details.includes(item.id)}>\n                  <CCardHeader>\n                    <h5>Thông tin chi nhánh khách hàng</h5>\n                  </CCardHeader>\n                  <CCardBody>\n                    <dl className=\"row\">\n                      <dt className=\"col-sm-2\">Mã chi nhánh:</dt>\n                      <dd className=\"col-sm-9\">{item.code}</dd>\n                    </dl>\n                    <dl className=\"row\">\n                      <dt className=\"col-sm-2\">Tên chi nhánh:</dt>\n                      <dd className=\"col-sm-9\">{item.name}</dd>\n                    </dl>\n                    <dl className=\"row\">\n                      <dt className=\"col-sm-2\">Mô tả:</dt>\n                      <dd className=\"col-sm-9\">{item.description}</dd>\n                    </dl>\n                  </CCardBody>\n                </CCollapse>\n              );\n            },\n          }}\n        />\n        <CPagination\n          activePage={activePage}\n          pages={Math.floor(initialState.totalItem / size) + 1}\n          onActivePageChange={i => setActivePage(i)}\n        />\n      </CCardBody>\n    </CCard>\n  );\n};\n\nexport default Branch;\n"]}]}