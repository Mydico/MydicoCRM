opdownTasks = () => {\n  const itemsCount = 5;\n  return (\n    <CDropdown inNav className=\"c-header-nav-item mx-2\">\n      <CDropdownToggle className=\"c-header-nav-link\" caret={false}>\n        <CIcon name=\"cil-list\" />\n        <CBadge shape=\"pill\" color=\"warning\">\n          {itemsCount}\n        </CBadge>\n      </CDropdownToggle>\n      <CDropdownMenu placement=\"bottom-end\" className=\"pt-0\">\n        <CDropdownItem header tag=\"div\" className=\"text-center\" color=\"light\">\n          <strong>You have {itemsCount} pending tasks</strong>\n        </CDropdownItem>\n        <CDropdownItem className=\"d-block\">\n          <div className=\"small mb-1\">\n            Upgrade NPM &amp; Bower{' '}\n            <span className=\"float-right\">\n              <strong>0%</strong>\n            </span>\n          </div>\n          <CProgress size=\"xs\" color=\"info\" value={0} />\n        </CDropdownItem>\n        <CDropdownItem className=\"d-block\">\n          <div className=\"small mb-1\">\n            ReactJS Version{' '}\n            <span className=\"float-right\">\n              <strong>25%</strong>\n            </span>\n          </div>\n          <CProgress size=\"xs\" color=\"danger\" value={25} />\n        </CDropdownItem>\n        <CDropdownItem className=\"d-block\">\n          <div className=\"small mb-1\">\n            VueJS Version{' '}\n            <span className=\"float-right\">\n              <strong>50%</strong>\n            </span>\n          </div>\n          <CProgress size=\"xs\" color=\"warning\" value={50} />\n        </CDropdownItem>\n        <CDropdownItem className=\"d-block\">\n          <div className=\"small mb-1\">\n            Add new layouts{' '}\n            <span className=\"float-right\">\n              <strong>75%</strong>\n            </span>\n          </div>\n          <CProgress size=\"xs\" color=\"info\" value={75} />\n        </CDropdownItem>\n        <CDropdownItem className=\"d-block\">\n          <div classN