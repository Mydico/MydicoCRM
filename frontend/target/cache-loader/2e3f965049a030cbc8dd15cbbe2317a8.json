AjBV,EAiBsB,UAACX,KAAD,EAAQY,MAAR,EAAmB;AAClDrB,IAAAA,iBAAiB,CAACY,MAAlB,CAAyBH,KAAzB,EAAgCY,MAAM,CAACE,OAAvC;AACAd,IAAAA,KAAK,CAACb,YAAN,CAAmBC,OAAnB,GAA6B,KAA7B;AACD,GApBU,mCAqBVL,aAAa,CAAC4B,SArBJ,EAqBgB,UAACX,KAAD,EAAQY,MAAR,EAAmB;AAC5CrB,IAAAA,iBAAiB,CAACwB,MAAlB,CAAyBf,KAAzB,EAAgCY,MAAM,CAACE,OAAP,CAAeE,IAA/C;AACAhB,IAAAA,KAAK,CAACb,YAAN,CAAmBG,SAAnB,GAA+BsB,MAAM,CAACE,OAAP,CAAeG,KAA9C;AACAjB,IAAAA,KAAK,CAACb,YAAN,CAAmBC,OAAnB,GAA6B,KAA7B;AACD,GAzBU,mCA0BVH,iBAAiB,CAAC0B,SA1BR,EA0BoB,UAACX,KAAD,EAAQY,MAAR,EAAmB;AAChDrB,IAAAA,iBAAiB,CAACwB,MAAlB,CAAyBf,KAAzB,EAAgCY,MAAM,CAACE,OAAP,CAAeE,IAA/C;AACAhB,IAAAA,KAAK,CAACb,YAAN,CAAmBC,OAAnB,GAA6B,KAA7B;AACD,GA7BU,mCA8BVL,aAAa,CAAC8B,QA9BJ,EA8Be,UAACb,KAAD,EAAQY,MAAR,EAAmB;AAC3CZ,IAAAA,KAAK,CAACZ,OAAN,GAAgB,KAAhB;AACD,GAhCU;AAhBW,CAAD,CAAzB;AAoDA,eAAeO,KAAK,CAACuB,OAArB;qBACmCvB,KAAK,CAACwB,O;IAA1BpB,Q,kBAAAA,Q;IAAUE,K,kBAAAA,K;;AAEzB,OAAO,IAAMmB,6BAA6B,GAAG7B,iBAAiB,CAAC8B,YAAlB,CAA+B,UAAArB,KAAK;AAAA,SAAIA,KAAK,CAACP,UAAV;AAAA,CAApC,CAAtC","sourcesContent":["import { createEntityAdapter, createSelector, createSlice } from '@reduxjs/toolkit';\nimport { creatingDepartment, getDepartment, getDetailDepartment, getTreeDepartment, updateDepartment } from './department.api';\n\nconst initialState = {\n  loading: false,\n  updatingSuccess: false,\n  totalItem: 0\n};\n\nexport const departmentAdapter = createEntityAdapter({\n  // Assume IDs are stored in a field other than `book.id`\n  selectId: department => department.id\n  // Keep the \"all IDs\" array sorted based on book titles\n  // sortComparer: (a, b) => a.login.localeCompare(b.login),\n});\n\nconst slice = createSlice({\n  name: 'department',\n  initialState: departmentAdapter.getInitialState({ initialState }),\n  reducers: {\n    fetching(state) {\n      state.initialState.loading = true;\n    },\n    reset(state) {\n      state.initialState.loading = false;\n      state.initialState.updatingSuccess = false;\n    },\n    departmentAddOne: departmentAdapter.addOne,\n    departmentAddMany: departmentAdapter.addMany,\n    departmentUpdate: departmentAdapter.updateOne,\n    departmentRemove: departmentAdapter.removeOne\n  },\n  extraReducers: {\n    [creatingDepartment.fulfilled]: (state, action) => {\n      state.initialState.updatingSuccess = true;\n      state.initialState.loading = false;\n    },\n    [creatingDepartment.rejected]: (state, action) => {\n      state.initialState.updatingSuccess = false;\n      state.initialState.loading = false;\n    },\n    [updateDepartment.fulfilled]: (state, action) => {\n      state.initialState.updatingSuccess = true;\n      state.initialState.loading = false;\n    },\n    [updateDepartment.rejected]: (state, action) => {\n      state.initialState.updatingSuccess = false;\n      state.initialState.loading = false;\n    },\n    [getDetailDepartment.fulfilled]: (state, action) => {\n      departmentAdapter.addOne(state, action.payload);\n      state.initialState.loading = false;\n    },\n    [getDepartment.fulfilled]: (state, action) => {\n      departmentAdapter.setAll(state, action.payload.data);\n      state.initialState.totalItem = action.payload.total;\n      state.initialState.loading = false;\n    },\n    [getTreeDepartment.fulfilled]: (state, action) => {\n      departmentAdapter.setAll(state, action.payload.data);\n      state.initialState.loading = false;\n    },\n    [getDepartment.rejected]: (state, action) => {\n      state.loading = false;\n    }\n  }\n});\n\nexport default slice.reducer;\nexport const { fetching, reset } = slice.actions;\n\nexport const globalizedDepartmentSelectors = departmentAdapter.getSelectors(state => state.department);\n"]}]}