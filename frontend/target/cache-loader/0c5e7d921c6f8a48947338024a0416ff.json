,CAAChB,QAAQ,EAAT,CAAR;AACAgB,IAAAA,QAAQ,CAACjB,oBAAoB,CAAC0B,MAAD,CAArB,CAAR;AACD,GAHD;;AAKA/C,EAAAA,SAAS,CAAC,YAAM;AACd,QAAIoC,YAAY,CAACY,eAAjB,EAAkC;AAChCT,MAAAA,OAAO,CAACU,MAAR;AACD;AACF,GAJQ,EAIN,CAACb,YAAY,CAACY,eAAd,CAJM,CAAT;AAMA,sBACE,oBAAC,KAAD,qBACE,oBAAC,WAAD,qBACE,oBAAC,UAAD,8BADF,CADF,eAIE,oBAAC,SAAD,qBACE,oBAAC,MAAD;AAAQ,IAAA,aAAa,EAAEJ,UAAU,IAAIP,aAArC;AAAoD,IAAA,kBAAkB,MAAtE;AAAuE,IAAA,QAAQ,EAAEa,QAAQ,CAAC1B,gBAAD,CAAzF;AAA6G,IAAA,QAAQ,EAAEsB;AAAvH,KACG;AAAA,QACCC,MADD,SACCA,MADD;AAAA,QAECI,MAFD,SAECA,MAFD;AAAA,QAKCC,YALD,SAKCA,YALD;AAAA,QAMCC,UAND,SAMCA,UAND;AAAA,QAOCC,YAPD,SAOCA,YAPD;AAAA,wBAYC,oBAAC,KAAD;AAAO,MAAA,QAAQ,EAAEA,YAAjB;AAA+B,MAAA,UAAU,MAAzC;AAA0C,MAAA,IAAI,EAAC;AAA/C,oBACE,oBAAC,IAAD,qBACE,oBAAC,IAAD;AAAM,MAAA,EAAE,EAAC;AAAT,oBACE,oBAAC,UAAD,qBACE,oBAAC,MAAD;AAAQ,MAAA,OAAO,EAAC;AAAhB,mCADF,eAEE,oBAAC,MAAD;AACE,MAAA,IAAI,EAAC,MADP;AAEE,MAAA,IAAI,EAAC,MAFP;AAGE,MAAA,EAAE,EAAC,MAHL;AAIE,MAAA,WAAW,EAAC,2BAJd;AAKE,MAAA,YAAY,EAAC,aALf;AAME,MAAA,OAAO,EAAEH,MAAM,CAACpB,IANlB;AAOE,MAAA,QAAQ,MAPV;AAQE,MAAA,QAAQ,EAAEqB,YARZ;AASE,MAAA,MAAM,EAAEC,UATV;AAUE,MAAA,KAAK,EAAEN,MAAM,CAAChB;AAVhB,MAFF,eAcE,oBAAC,gBAAD,QAAmBoB,MAAM,CAACpB,IAA1B,CAdF,CADF,eAiBE,oBAAC,UAAD,qBACE,oBAAC,MAAD;AAAQ,MAAA,OAAO,EAAC;AAAhB,uBADF,eAEE,oBAAC,MAAD;AACE,MAAA,IAAI,EAAC,MADP;AAEE,MAAA,IAAI,EAAC,aAFP;AAGE,MAAA,EAAE,EAAC,aAHL;AAIE,MAAA,WAAW,EAAC,eAJd;AAKE,MAAA,OAAO,EAAEoB,MAAM,CAACxB,WALlB;AAME,MAAA,QAAQ,MANV;AAOE,MAAA,QAAQ,EAAEyB,YAPZ;AAQE,MAAA,MAAM,EAAEC,UARV;AASE,MAAA,KAAK,EAAEN,MAAM,CAACpB;AAThB,MAFF,eAaE,oBAAC,gBAAD,QAAmBwB,MAAM,CAACxB,WAA1B,CAbF,CAjBF,eAgCE,oBAAC,UAAD;AAAY,MAAA,SAAS,EAAC;AAAtB,oBACE,oBAAC,OAAD;AAAS,MAAA,IAAI,EAAC,QAAd;AAAuB,MAAA,KAAK,EAAC,SAA7B;AAAuC,MAAA,QAAQ,EAAES,YAAY,CAACmB;AAA9D,oBACE,oBAAC,KAAD;AAAO,MAAA,IAAI,EAAC;AAAZ,MADF,OAC6BnB,YAAY,CAACmB,OAAb,GAAuB,YAAvB,GAAsC,cADnE,CADF,CAhCF,CADF,CADF,CAZD;AAAA,GADH,CADF,CAJF,CADF;AAkED,CAnGD;;AAqGA,eAAevB,oBAAf","sourcesContent":["import React, {useEffect, useState} from 'react';\nimport {\n  CButton,\n  CCard,\n  CCardHeader,\n  CCardBody,\n  CCol,\n  CForm,\n  CInvalidFeedback,\n  CFormGroup,\n  CLabel,\n  CInput,\n  CRow,\n\n  CCardTitle,\n} from '@coreui/react';\nimport CIcon from '@coreui/icons-react';\nimport {Formik} from 'formik';\nimport * as Yup from 'yup';\nimport {useDispatch, useSelector} from 'react-redux';\n\n\nimport {useHistory} from 'react-router-dom';\nimport {getDetailCustomerStatus, updateCustomerStatus} from './customer-status.api';\nimport {fetching, globalizedcustomerStatuselectors} from './customer-status.reducer';\n\n\nconst validationSchema = function() {\n  return Yup.object().shape({\n    description: Yup.string()\n        .min(5, `Mô tả liên lạc phải lớn hơn 5 kí tự`)\n        .required('Tên liên lạc không để trống'),\n    name: Yup.string()\n        .min(5, `Tên phải lớn hơn 5 kí tự`)\n        .required('Tên không để trống'),\n  });\n};\n\nconst CreateCustomerStatus = (props) => {\n  const {initialState} = useSelector((state) => state.customerStatus);\n  const initialValues = {\n    name: '',\n    description: '',\n  };\n\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const {selectById} = globalizedcustomerStatuselectors;\n  const customerStatus = useSelector((state) => selectById(state, props.match.params.id));\n\n  const [initValues, setInitValues] = useState(null);\n\n  useEffect(() => {\n    dispatch(getDetailCustomerStatus(props.match.params.id));\n  }, []);\n\n  useEffect(() => {\n    setInitValues(customerStatus);\n  }, [customerStatus]);\n\n  const onSubmit = (values, {}) => {\n    dispatch(fetching());\n    dispatch(updateCustomerStatus(values));\n  };\n\n  useEffect(() => {\n    if (initialState.updatingSuccess) {\n      history.goBack();\n    }\n  }, [initialState.updatingSuccess]);\n\n  return (\n    <CCard>\n      <CCardHeader>\n        <CCardTitle>Chỉnh sửa</CCardTitle>\n      </CCardHeader>\n      <CCardBody>\n        <Formik initialValues={initValues || initialValues} enableReinitialize validate={validate(validationSchema)} onSubmit={onSubmit}>\n          {({\n            values,\n            errors,\n\n\n            handleChange,\n            handleBlur,\n            handleSubmit\n\n\n            ,\n          }) => (\n            <CForm onSubmit={handleSubmit} noValidate name=\"simpleForm\">\n              <CRow>\n                <CCol lg=\"6\">\n                  <CFormGroup>\n                    <CLabel htmlFor=\"lastName\">Tên trạng thái</CLabel>\n                    <CInput\n                      type=\"text\"\n                      name=\"name\"\n                      id=\"name\"\n                      placeholder=\"Tên trạng thái\"\n                      autoComplete=\"family-name\"\n                      invalid={errors.name}\n                      required\n                      onChange={handleChange}\n                      onBlur={handleBlur}\n                      value={values.name}\n                    />\n                    <CInvalidFeedback>{errors.name}</CInvalidFeedback>\n                  </CFormGroup>\n                  <CFormGroup>\n                    <CLabel htmlFor=\"userName\">Mô tả</CLabel>\n                    <CInput\n                      type=\"text\"\n                      name=\"description\"\n                      id=\"description\"\n                      placeholder=\"Mô tả\"\n                      invalid={errors.description}\n                      required\n                      onChange={handleChange}\n                      onBlur={handleBlur}\n                      value={values.description}\n                    />\n                    <CInvalidFeedback>{errors.description}</CInvalidFeedback>\n                  </CFormGroup>\n                  <CFormGroup className=\"d-flex justify-content-center\">\n                    <CButton type=\"submit\" color=\"primary\" disabled={initialState.loading}>\n                      <CIcon name=\"cil-save\" /> {initialState.loading ? 'Đang xử lý' : 'Lưu thay đổi'}\n                    </CButton>\n                  </CFormGroup>\n                </CCol>\n              </CRow>\n            </CForm>\n          )}\n        </Formik>\n      </CCardBody>\n    </CCard>\n  );\n};\n\nexport default CreateCustomerStatus;\n"]}]}