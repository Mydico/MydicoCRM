AD,EAAMC,OAAN;AAAA,WAAkBD,GAAG,GAAIC,OAAO,CAACC,SAAR,GAAoBD,OAAO,CAACE,QAA5B,GAAuCF,OAAO,CAACI,aAAhD,GAAiE,GAAzF;AAAA,GADT,EACuG,CADvG,EAEE0B,cAFF,CAEiB,OAFjB,EAE0B;AAAEC,IAAAA,KAAK,EAAE,UAAT;AAAqBC,IAAAA,QAAQ,EAAE;AAA/B,GAF1B,MAEqE,EAHxE,CAJF,CAXF,eAqBE,6CACE;AAAI,IAAA,SAAS,EAAC;AAAd,kBACE,8DADF,CADF,eAIE;AAAI,IAAA,SAAS,EAAC;AAAd,kBACE,oCACG,CAAAvC,OAAO,SAAP,IAAAA,OAAO,WAAP,YAAAA,OAAO,CAAEI,YAAT,CACEC,MADF,CAEG,UAACC,GAAD,EAAMC,OAAN;AAAA,WACED,GAAG,IACFC,OAAO,CAACC,SAAR,GAAoBD,OAAO,CAACE,QAA5B,GAAwCF,OAAO,CAACC,SAAR,GAAoBD,OAAO,CAACE,QAA5B,GAAuCF,OAAO,CAACI,aAAhD,GAAiE,GADtG,CADL;AAAA,GAFH,EAKG,CALH,EAOE0B,cAPF,CAOiB,OAPjB,EAO0B;AAAEC,IAAAA,KAAK,EAAE,UAAT;AAAqBC,IAAAA,QAAQ,EAAE;AAA/B,GAP1B,MAOqE,EARxE,CADF,CAJF,CArBF,CADF,CADF,CAJF,CAnEF,eAmHE,oBAAC,IAAD;AAAM,IAAA,SAAS,EAAC;AAAhB,kBACE,oBAAC,OAAD;AAAS,IAAA,IAAI,EAAC,IAAd;AAAmB,IAAA,SAAS,EAAC,wBAA7B;AAAsD,IAAA,OAAO,EAAE;AAAA,aAAM3C,OAAO,CAAC6C,MAAR,EAAN;AAAA;AAA/D,qBADF,eAIE,oBAAC,OAAD;AAAS,IAAA,IAAI,EAAC,QAAd;AAAuB,IAAA,IAAI,EAAC,IAA5B;AAAiC,IAAA,SAAS,EAAC,sBAA3C;AAAkE,IAAA,QAAQ,EAAE1C,YAAY,CAAC2C,OAAzF;AAAkG,IAAA,OAAO,EAAExC;AAA3G,KACGH,YAAY,CAAC2C,OAAb,gBAAuB,oBAAC,QAAD;AAAU,IAAA,IAAI,EAAC;AAAf,IAAvB,GAAgD,eADnD,CAJF,CAnHF,CAPF,CADF,CADF;AAwID,CA5KD;;AA8KA,eAAejD,OAAf","sourcesContent":["/* eslint-disable jsx-a11y/anchor-is-valid */\nimport React, { useEffect, useState } from 'react';\nimport { Table } from 'reactstrap';\nimport { useHistory, useLocation } from 'react-router-dom';\nimport { creatingOrder } from './order.api';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { reset, fetching } from './order.reducer';\n\nimport { CButton, CCard, CCardHeader, CCardBody, CCol, CRow } from '@coreui/react/lib';\nimport { CSpinner } from '@coreui/react';\nimport cities from '../../../../shared/utils/city';\nimport district from '../../../../shared/utils/district';\nconst Invoice = () => {\n  const location = useLocation();\n  const dispatch = useDispatch();\n  const history = useHistory();\n\n  const { initialState } = useSelector(state => state.order);\n\n  const [invoice, setInvoice] = useState(null);\n  useEffect(() => {\n    if (location.state) setInvoice(location.state);\n  }, []);\n\n  const onCreateOrder = () => {\n    if (invoice) {\n      invoice.totalMoney = invoice?.orderDetails.reduce((sum, current) => sum + current.priceReal * current.quantity, 0);\n      invoice.realMoney = invoice?.orderDetails.reduce(\n        (sum, current) =>\n          sum + (current.priceReal * current.quantity - (current.priceReal * current.quantity * current.reducePercent) / 100),\n        0\n      );\n      invoice.reduceMoney = invoice?.orderDetails.reduce(\n        (sum, current) => sum + (current.priceReal * current.quantity * current.reducePercent) / 100,\n        0\n      );\n      dispatch(fetching());\n      dispatch(creatingOrder(invoice));\n    }\n  };\n  useEffect(() => {\n    if (initialState.updatingSuccess) {\n      dispatch(reset());\n      localStorage.setItem('order', JSON.stringify({}));\n      history.push('/orders');\n    }\n  }, [initialState.updatingSuccess]);\n\n  return (\n    <div className=\"animated fadeIn\">\n      <CCard>\n        <CCardHeader>\n          Đơn hàng <strong>#{invoice?.code}</strong>\n          <a href=\"#\" className=\"btn btn-sm btn-secondary mr-1 float-right\">\n            <i className=\"fa fa-print\"></i> In\n          </a>\n        </CCardHeader>\n        <CCardBody>\n          <CRow className=\"mb-4\">\n            <CCol sm=\"4\">\n              <h6 className=\"mb-3\">Từ:</h6>\n              <div>\n                <strong>Công ty TNHH Thương mại và Dịch vụ Mỹ Đình</strong>\n              </div>\n              <div>Địa chỉ: P301, Nhà CT5, KĐT Mỹ Đình, Mễ Trì, Phường Mỹ Đình 1, Quận Nam Từ Liêm, Hà Nội.</div>\n              <div>Phone: 0243 86 89 205</div>\n            </CCol>\n            <CCol sm=\"4\">\n              <h6 className=\"mb-3\">Tới:</h6>\n              <div>\n                <strong>{invoice?.customer.name || ''}</strong>\n              </div>\n              <div>{invoice?.address || ''}</div>\n              <div>{`${district.filter(dist => dist.value === invoice?.customer?.district)[0]?.label || ''}, ${cities.filter(\n                city => city.value === invoice?.customer?.city\n              )[0]?.label || ''}`}</div>{' '}\n              <div>Phone: {invoice?.customer.tel || ''}</div>\n            </CCol>\n            <CCol sm=\"4\">\n              <h6 className=\"mb-3\">Chương trình bán hàng:</h6>\n              <div>\n                <strong> {invoice?.promotion?.name}</strong>\n              </div>\n              <div>Loại khách hàng: {invoice?.promotion?.customerType?.name}</div>\n            </CCol>\n          </CRow>\n          <Table striped responsive>\n            <thead>\n              <tr>\n                <th className=\"center\">#</th>\n                <th>Tên sản phẩm</th>\n                <th>Dung tích</th>\n                <th className=\"center\">Số lượng</th>\n                <th className=\"right\">Đơn giá</th>\n                <th className=\"right\">Chiết khấu(%)</th>\n                <th className=\"right\">Tổng</th>\n              </tr>\n            </thead>\n            <tbody>\n              {invoice?.orderDetails.map((item, index) => {\n                return (\n                  <tr key={index}>\n                    <td>{index + 1}</td>\n                    <td>{item.product?.name}</td>\n                    <td>{item.product?.volume}</td>\n                    <td>{item.quantity}</td>\n\n                    <td>{Number(item.priceReal).toLocaleString('it-IT', { style: 'currency', currency: 'VND' }) || ''}</td>\n                    <td>{item.reducePercent}%</td>\n                    <td>{(item.priceReal * item.quantity).toLocaleString('it-IT', { style: 'currency', currency: 'VND' }) || ''}</td>\n                    <td>\n                      {(item.priceReal * item.quantity - (item.priceReal * item.quantity * item.reducePercent) / 100).toLocaleString(\n                        'it-IT',\n                        {\n                          style: 'currency',\n                          currency: 'VND'\n                        }\n                      ) || ''}\n                    </td>\n                  </tr>\n                );\n              })}\n            </tbody>\n          </Table>\n          <CRow>\n            <CCol lg=\"4\" sm=\"5\">\n              Ghi chú: <strong>{invoice?.note}</strong>\n            </CCol>\n            <CCol lg=\"4\" sm=\"5\" className=\"ml-auto\">\n              <Table className=\"table-clear\">\n                <tbody>\n                  <tr>\n                    <td className=\"left\">\n                      <strong>Tổng tiền</strong>\n                    </td>\n                    <td className=\"right\">\n                      {invoice?.orderDetails\n                        .reduce((sum, current) => sum + current.priceReal * current.quantity, 0)\n                        .toLocaleString('it-IT', { style: 'currency', currency: 'VND' }) || ''}\n                    </td>\n                  </tr>\n                  <tr>\n                    <td className=\"left\">\n                      <strong>Chiết khấu</strong>\n                    </td>\n                    <td className=\"right\">\n                      {invoice?.orderDetails\n                        .reduce((sum, current) => sum + (current.priceReal * current.quantity * current.reducePercent) / 100, 0)\n                        .toLocaleString('it-IT', { style: 'currency', currency: 'VND' }) || ''}\n                    </td>\n                  </tr>\n                  <tr>\n                    <td className=\"left\">\n                      <strong>Tiền thanh toán</strong>\n                    </td>\n                    <td className=\"right\">\n                      <strong>\n                        {invoice?.orderDetails\n                          .reduce(\n                            (sum, current) =>\n                              sum +\n                              (current.priceReal * current.quantity - (current.priceReal * current.quantity * current.reducePercent) / 100),\n                            0\n                          )\n                          .toLocaleString('it-IT', { style: 'currency', currency: 'VND' }) || ''}\n                      </strong>\n                    </td>\n                  </tr>\n                </tbody>\n              </Table>\n            </CCol>\n          </CRow>\n          <CRow className=\"d-flex justify-content-between mr-5\">\n            <CButton size=\"lg\" className=\"btn btn-secondary ml-3\" onClick={() => history.goBack()}>\n              Quay lại\n            </CButton>\n            <CButton type=\"submit\" size=\"lg\" className=\"btn btn-success mr-5\" disabled={initialState.loading} onClick={onCreateOrder}>\n              {initialState.loading ? <CSpinner size=\"sm\" /> : ' Tạo đơn hàng'}\n            </CButton>\n          </CRow>\n        </CCardBody>\n      </CCard>\n    </div>\n  );\n};\n\nexport default Invoice;\n"]}]}