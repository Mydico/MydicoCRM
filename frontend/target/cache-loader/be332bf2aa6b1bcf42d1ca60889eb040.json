ector(userSafeSelector);\n  const show = useSelector(state => state.app.asideShow);\n  const setState = state => dispatch(setAsideShow(state));\n\n  const onLogout = () => {\n    dispatch(logout());\n  };\n  return (\n    <CDropdown inNav className=\"c-header-nav-items mx-2\" direction=\"down\">\n\n      <CDropdownToggle className=\"c-header-nav-link\" caret={false}>\n        <div className=\"c-avatar\">\n          <CImg src={'avatars/6.jpg'} className=\"c-avatar-img\" alt=\"admin@bootstrapmaster.com\" />\n        </div>\n      </CDropdownToggle>\n      <CDropdownMenu className=\"pt-0\" placement=\"bottom-end\">\n        <CDropdownItem header tag=\"div\" color=\"light\" className=\"text-center\">\n        Xin chào <strong>{account.firstName} {account.lastName}</strong>\n        </CDropdownItem>\n        <CDropdownItem onClick={() =>  setState(true)}>\n          <CIcon name=\"cil-user\" className=\"mfe-2\" />\n          Thông tin cá nhân\n        </CDropdownItem>\n        <CDropdownItem divider />\n        <CDropdownItem onClick={onLogout}>\n          <CIcon name=\"cil-lock-locked\" className=\"mfe-2\" />\n          Đăng xuất\n        </CDropdownItem>\n      </CDropdownMenu>\n    </CDropdown>\n  );\n};\n\nexport default TheHeaderDropdown;\n"]}]}