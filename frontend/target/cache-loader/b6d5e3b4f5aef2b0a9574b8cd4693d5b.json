4BACE,oBAAC,SAAD;AAAW,UAAA,IAAI,EAAEhC,QAAO,CAACsD,QAAR,CAAiBtB,IAAI,CAACqB,EAAtB;AAAjB,wBACE,oBAAC,SAAD,qBACE,0EADF,eAEE,oBAAC,IAAD;AAAM,UAAA,SAAS,EAAC;AAAhB,wBACE,oBAAC,IAAD;AAAM,UAAA,EAAE,EAAC;AAAT,wBACE;AAAI,UAAA,SAAS,EAAC;AAAd,wBACE;AAAI,UAAA,SAAS,EAAC;AAAd,+CADF,eAEE;AAAI,UAAA,SAAS,EAAC;AAAd,WAA0BrB,IAAI,CAACuB,IAA/B,CAFF,CADF,eAKE;AAAI,UAAA,SAAS,EAAC;AAAd,wBACE;AAAI,UAAA,SAAS,EAAC;AAAd,gDADF,eAEE;AAAI,UAAA,SAAS,EAAC;AAAd,WAA0BvB,IAAI,CAACwB,IAA/B,CAFF,CALF,eASE;AAAI,UAAA,SAAS,EAAC;AAAd,wBACE;AAAI,UAAA,SAAS,EAAC;AAAd,4BADF,eAEE;AAAI,UAAA,SAAS,EAAC;AAAd,WAA0BxB,IAAI,CAACyB,WAA/B,CAFF,CATF,CADF,CAFF,CADF,CADF;AAuBD;AA5DU;AAnBf,IAJF,eAsFE,oBAAC,WAAD;AACE,IAAA,UAAU,EAAEpD,UADd;AAEE,IAAA,KAAK,EAAEqD,IAAI,CAACC,KAAL,CAAWvD,YAAY,CAACwD,SAAb,GAAyBrD,IAApC,IAA4C,CAFrD;AAGE,IAAA,kBAAkB,EAAE,4BAAAsD,CAAC;AAAA,aAAIvD,aAAa,CAACuD,CAAD,CAAjB;AAAA;AAHvB,IAtFF,CAVF,CADF;AAyGD,CArLD;;AAuLA,eAAe/D,YAAf","sourcesContent":["import React, { useEffect, useState } from 'react';\nimport { CCardBody, CBadge, CButton, CCollapse, CDataTable, CCard, CCardHeader, CRow, CCol, CPagination, CImg } from '@coreui/react';\nimport CIcon from '@coreui/icons-react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { getProductBrand } from './product-brand.api';\nimport { globalizedproductBrandsSelectors, reset } from './product-brand.reducer';\nimport { useHistory } from 'react-router-dom';\n\nconst ProductBrand = props => {\n  const [details, setDetails] = useState([]);\n  const { initialState } = useSelector(state => state.productBrand);\n  const [activePage, setActivePage] = useState(1);\n  const [size, setSize] = useState(20);\n  const dispatch = useDispatch();\n  const history = useHistory();\n  useEffect(() => {\n    dispatch(reset());\n  }, []);\n  const { selectAll } = globalizedproductBrandsSelectors;\n  const productBrandss = useSelector(selectAll);\n  useEffect(() => {\n    dispatch(getProductBrand({ page: activePage - 1, size: size, sort: 'createdDate,desc' }));\n  }, [activePage, size]);\n\n  const toggleDetails = index => {\n    const position = details.indexOf(index);\n    let newDetails = details.slice();\n    if (position !== -1) {\n      newDetails.splice(position, 1);\n    } else {\n      newDetails = [...details, index];\n    }\n    setDetails(newDetails);\n  };\n\n  // Code\tTên cửa hàng/đại lý\tNgười liên lạc\tNăm Sinh\tĐiện thoại\tNhân viên quản lý\tthương hiệu\tPhân loại\tSửa\tTạo đơn\n  const fields = [\n    {\n      key: 'order',\n      label: 'STT',\n      _style: { width: '1%' },\n      filter: false\n    },\n    { key: 'code', label: 'Mã thương hiệu', _style: { width: '15%' } },\n    { key: 'name', label: 'Tên thương hiệu', _style: { width: '15%' } },\n    { key: 'description', label: 'Mô tả', _style: { width: '15%' } },\n    {\n      key: 'show_details',\n      label: '',\n      _style: { width: '1%' },\n      filter: false\n    }\n  ];\n\n  const getBadge = status => {\n    switch (status) {\n      case 'Active':\n        return 'success';\n      case 'Inactive':\n        return 'secondary';\n      case 'Pending':\n        return 'warning';\n      case 'Banned':\n        return 'danger';\n      default:\n        return 'primary';\n    }\n  };\n  const csvContent = productBrandss.map(item => Object.values(item).join(',')).join('\\n');\n  const csvCode = 'data:text/csv;charset=utf-8,SEP=,%0A' + encodeURIComponent(csvContent);\n  const toCreateCustomer = () => {\n    history.push(`${props.match.url}/new`);\n  };\n\n  const onFilterColumn = value => {\n    if (Object.keys(value).length > 0) {\n      dispatch(getProductBrand({ page: 0, size: size, sort: 'createdDate,desc', ...value }));\n    }\n  };\n\n  const toEditProductBrand = typeId => {\n    history.push(`${props.match.url}/${typeId}/edit`);\n  };\n\n  return (\n    <CCard>\n      <CCardHeader>\n        <CIcon name=\"cil-grid\" /> Danh sách thương hiệu\n        {/* <CButton color=\"primary\" className=\"mb-2\">\n         Thêm mới khách hàng\n        </CButton> */}\n        <CButton color=\"success\" variant=\"outline\" className=\"ml-3\" onClick={toCreateCustomer}>\n          <CIcon name=\"cil-plus\" /> Thêm mới\n        </CButton>\n      </CCardHeader>\n      <CCardBody>\n        <CButton color=\"primary\" className=\"mb-2\" href={csvCode} download=\"customertypes.csv\" target=\"_blank\">\n          Tải excel (.csv)\n        </CButton>\n        <CDataTable\n          items={productBrandss}\n          fields={fields}\n          columnFilter\n          tableFilter\n          cleaner\n          itemsPerPageSelect={{ label: 'Số lượng trên một trang', values: [20, 30, 50] }}\n          itemsPerPage={size}\n          hover\n          sorter\n          // loading\n          // onRowClick={(item,index,col,e) => console.log(item,index,col,e)}\n          onPageChange={val => console.log('new page:', val)}\n          onPagesChange={val => console.log('new pages:', val)}\n          onPaginationChange={val => setSize(val)}\n          // onFilteredItemsChange={(val) => console.log('new filtered items:', val)}\n          // onSorterValueChange={(val) => console.log('new sorter value:', val)}\n          onTableFilterChange={val => console.log('new table filter:', val)}\n          onColumnFilterChange={onFilterColumn}\n          scopedSlots={{\n            order: (item, index) => <td>{index + 1}</td>,\n            status: item => (\n              <td>\n                <CBadge color={getBadge(item.status)}>{item.status}</CBadge>\n              </td>\n            ),\n            show_details: item => {\n              return (\n                <td className=\"py-2 d-flex\">\n                  <CButton\n                    color=\"primary\"\n                    variant=\"outline\"\n                    shape=\"square\"\n                    size=\"sm\"\n                    className=\"mr-3\"\n                    onClick={() => {\n                      toEditProductBrand(item.id);\n                    }}\n                  >\n                    <CIcon name=\"cil-pencil\" />\n                  </CButton>\n                  <CButton\n                    color=\"primary\"\n                    variant=\"outline\"\n                    shape=\"square\"\n                    size=\"sm\"\n                    onClick={() => {\n                      toggleDetails(item.id);\n                    }}\n                  >\n                    <CIcon name=\"cilZoom\" />\n                  </CButton>\n                </td>\n              );\n            },\n            details: item => {\n              return (\n                <CCollapse show={details.includes(item.id)}>\n                  <CCardBody>\n                    <h5>Thông tin thương hiệu</h5>\n                    <CRow className=\"mt-5\">\n                      <CCol lg=\"6\">\n                        <dl className=\"row\">\n                          <dt className=\"col-sm-3\">Mã thương hiệu:</dt>\n                          <dd className=\"col-sm-9\">{item.code}</dd>\n                        </dl>\n                        <dl className=\"row\">\n                          <dt className=\"col-sm-3\">Tên thương hiệu:</dt>\n                          <dd className=\"col-sm-8\">{item.name}</dd>\n                        </dl>\n                        <dl className=\"row\">\n                          <dt className=\"col-sm-3\">Mô tả:</dt>\n                          <dd className=\"col-sm-9\">{item.description}</dd>\n                        </dl>\n                      </CCol>\n                    </CRow>\n                  </CCardBody>\n                </CCollapse>\n              );\n            }\n          }}\n        />\n        <CPagination\n          activePage={activePage}\n          pages={Math.floor(initialState.totalItem / size) + 1}\n          onActivePageChange={i => setActivePage(i)}\n        />\n      </CCardBody>\n    </CCard>\n  );\n};\n\nexport default ProductBrand;\n"]}]}