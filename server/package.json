{
  "name": "MydicoCRM",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.build.json && npm run copy-resources",
    "webpack:prod": "npm run clean && webpack --config webpack.server.prod.config.js && npm run copy-resources",
    "copy-resources": "ts-node scripts/copy-resources.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "sonar:scanner": "sonar-scanner",
    "start:node": "npm run copy-resources && ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "npm run copy-resources && tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "npm run copy-resources && tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "npm run copy-resources && node dist/main.js",
    "start": "npm run clean && npm run copy-resources && nodemon --watch src -e ts,tsx --exec ts-node src/main.ts",
    "lint": "eslint . --ignore-path .server.eslintignore --no-eslintrc --config .server.eslintrc.json --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint:fix && npm run lint && jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --coverage --config ./e2e/jest.e2e.config.json"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^1.0.0",
    "@nestjs/axios": "0.0.2",
    "@nestjs/common": "7.5.1",
    "@nestjs/core": "7.5.1",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/passport": "7.1.0",
    "@nestjs/platform-express": "7.5.1",
    "@nestjs/platform-fastify": "^7.5.1",
    "@nestjs/platform-socket.io": "^8.0.6",
    "@nestjs/schedule": "^1.0.0",
    "@nestjs/serve-static": "2.1.4",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "7.1.4",
    "@nestjs/websockets": "^8.0.6",
    "cache-manager": "^3.4.3",
    "cache-manager-redis-store": "^2.0.0",
    "casbin": "^5.11.3",
    "class-transformer": "0.3.1",
    "class-validator": "0.13.1",
    "cloud-config-client": "1.4.2",
    "cluster": "^0.7.7",
    "compression": "^1.7.4",
    "csvtojson": "^2.0.10",
    "dotenv": "8.2.0",
    "eureka-js-client": "4.5.0",
    "express-list-endpoints": "^5.0.0",
    "faker": "^5.1.0",
    "fast-memoize": "^2.5.2",
    "fastify-compress": "^3.4.2",
    "fastify-multer": "^2.0.2",
    "fastify-multipart": "^4.0.7",
    "fastify-swagger": "^4.7.0",
    "helmet": "^4.4.1",
    "ioredis": "^4.27.4",
    "js-yaml": "3.13.1",
    "lodash": "^4.17.14",
    "mysql2": "^2.2.5",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "resize-optimize-images": "^1.1.2",
    "rxjs": "^6.5.2",
    "shelljs": "0.8.3",
    "swagger-ui-express": "^4.0.7",
    "typeorm": "^0.2.34",
    "typeorm-adapter": "^1.4.0",
    "typeorm-encrypted": "0.5.6",
    "utils-decorators": "^1.10.1",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@nestjs/testing": "7.5.1",
    "@types/cron": "^1.7.3",
    "@types/express": "4.17.1",
    "@types/express-serve-static-core": "4.17.3",
    "@types/jest": "25.2.3",
    "@types/node": "11.13.4",
    "@types/passport-jwt": "3.0.1",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.11.0",
    "@typescript-eslint/eslint-plugin-tslint": "2.11.0",
    "@typescript-eslint/parser": "2.11.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-loader": "3.0.3",
    "jest": "25.2.3",
    "json-loader": "^0.5.7",
    "nodemon": "1.19.1",
    "prettier": "1.17.0",
    "rimraf": "3.0.0",
    "sonarqube-scanner": "2.5.0",
    "supertest": "4.0.2",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "25.2.1",
    "ts-loader": "^9.0.0",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.20.1",
    "typescript": "3.8.3",
    "webpack-cli": "^4.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": "(/test/.*|\\.(spec))\\.(ts|js)$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  }
}
